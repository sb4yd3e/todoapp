{
  "name": "TodoApp",
  "version": "1.0.0",
   "private": true,
  "repository": {
	  "type": "git",
	  "url": "git://github.com/sb4yd3e/todoapp.git"
	},
  "description": "express react redux neo4j",
  "main": "index.js",
  "scripts": {
    "prod": "npm run serve",
    "dev": "npm run watch:js & npm run watch:sass & nodemon bin/server.js",
    "serve": "node bin/server.js",
    "build:js": "browserify -t [ babelify --presets [ es2015 react stage-1 ] ] src/dev/index.js | uglifyjs -mc > src/public/js/bundle.js",
    "watch:js": "watchify src/dev/index.js -o src/public/js/bundle.js -t [ babelify --presets [ es2015 react stage-1 ] ] -v",
    "watch:sass": "node-sass -w src/dev/sass/style.scss -o src/public/css/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": "5.8.0",
    "npm": "3.7.3",
    "socket.io": "^1.4.5",
    "neo4j": "^2.0.0-RC2"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "author": "Carmelo Catalfamo",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.11.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.7.2",
    "babelify": "^7.2.0",
    "bcrypt": "^0.8.5",
    "body-parser": "^1.15.0",
    "browserify": "^13.0.0",
    "cookie-parser": "^1.4.1",
    "express": "^4.13.4",
    "express-handlebars": "^3.0.0",
    "joi": "^8.0.5",
    "jwt-simple": "^0.5.0",
    "lodash": "^4.7.0",
    "neo4j-transactions": "^1.0.2",
    "node-sass": "^3.4.2",
    "nodemon": "^1.9.1",
    "path": "^0.12.7",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "redux": "^3.3.1",
    "redux-promise": "^0.5.3",
    "shortid": "^2.2.4",
    "watchify": "^3.7.0",
    "underscore":"^1.8.3",
    "timekeeper": "0.0.5",
    "calendar": "^0.1.0",
    "moment": "^2.10.6",
    "moment-range": "^2.0.3",
    "calendar": "^0.1.0",
    "classnames": "^2.1.1",
    "immutable": "^3.7.2",
    "react-addons-pure-render-mixin": "^0.14.0"
  }
}
